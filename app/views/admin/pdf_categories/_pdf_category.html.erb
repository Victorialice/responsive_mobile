<tr>                                                                                             
  <th> 
    <%= pdf_category.name %> 
  </th>
  <th><%= pdf_category.parent_id.present? ? pdf_category.parent.try(:name) : "一级分类" %></th>
  <th><%= pdf_category.note %></th>
  <th><%= link_to "编辑", [:edit, :admin, pdf_category], class: "btn btn-primary" %>  <%= link_to "删除", [:admin, pdf_category], method: :delete, class: "btn btn-primary", data: { confirm: "确认删除吗?" } %></th>
</tr>                                                                                            
